namespace demo.translate.conditional_set_to.example_6
version "${project.version}"

/*
test 7.1 shouldSetFieldFromSynonymSetToMultipleWhen1,2,3
*/
synonym source CONDITIONAL_SET_TO_EXAMPLE_6

type Root:
    [rootType]
	xField X (1..1)
		[synonym CONDITIONAL_SET_TO_EXAMPLE_6 value "a"]

type X:
	yField Y (1..1)
		[synonym CONDITIONAL_SET_TO_EXAMPLE_6 value "b"]
	strField string (1..1)
		[synonym CONDITIONAL_SET_TO_EXAMPLE_6
			set to "FISH2" when "b->c->d" = "FISH",
			set to "SAUSAGE2" when "b->c->d" = "SAUSAGE",
			set to "DEFAULT"]

type Y:
	zField Z (1..1)
		[synonym CONDITIONAL_SET_TO_EXAMPLE_6 value "c"]

type Z:
	strField string (1..1)
		[synonym CONDITIONAL_SET_TO_EXAMPLE_6 value "d"]

