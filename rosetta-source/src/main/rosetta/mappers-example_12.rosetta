synonym source MAPPERS_EXAMPLE_12

type Root:
    [rootType]
	engine Engine (1..1)
	    [synonym MAPPERS_EXAMPLE_12 value "engineType"]

type Engine:
	engineSpecification EngineSpecification (0..*)
	    // merge lists b and e together
	    [synonym MAPPERS_EXAMPLE_12 value "engineDetail" mapper "Example12"]

type EngineSpecification:
    // value updated by the mapper
    emmissions string (1..1)
    // value updated by the mapper
    fuelType string (1..1)
    // value updated by the mapper
    volumeUnit string (1..1)
    // value updated by the mapper
    volumeConsumption string (1..1)
	yField Y (1..1)
	    [synonym MAPPERS_EXAMPLE_12 value "a"]

type Y:
	zField Z (0..*)
	    // merge lists b and e together
	    [synonym MAPPERS_EXAMPLE_12 value "b" mapper "Example12"]

type Z:
    // value updated by the mapper
    str1Field string (1..1)
    // value updated by the mapper
    str2Field string (1..1)
    // value updated by the mapper
    str3Field string (1..1)
    // value updated by the mapper
    str4Field string (1..1)

