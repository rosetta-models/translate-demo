namespace demo.translate.conditional_set.example_16
version "${project.version}"

synonym source CONDITIONAL_SET_EXAMPLE_16

type Root:
    [rootType]
	yField Y (1..1)
	    [synonym CONDITIONAL_SET_EXAMPLE_16 value "a"]

type Y:
	zField Z (1..*)

type Z:
	strField string (1..1) 
	    [metadata scheme]
