synonym source MAPPERS_EXAMPLE_13

type Root:
    [rootType]
	priceQuantity PriceQuantity (1..*)
	product Product (1..*)

type PriceQuantity:
	quantity Quantity (1..1)
        [metadata location]
        [synonym MAPPERS_EXAMPLE_13 value "b" path "a" mapper "Example13"]

type Product:
    resolvablePayoutQuantity ResolvablePayoutQuantity (1..1)
	    [synonym MAPPERS_EXAMPLE_13 value "b" path "a"]

type ResolvablePayoutQuantity:
	resolvedQuantity Quantity (1..1)
        [metadata address "pointsTo"=PriceQuantity->quantity]
        [synonym MAPPERS_EXAMPLE_13 meta "c"]

type Quantity:
	amount number (1..1)

