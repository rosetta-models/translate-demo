namespace demo.translate.mappers.example_5
version "${project.version}"

/*
$DESCRIPTION$
*/
synonym source MAPPERS_EXAMPLE_5

type Root:
    [rootType]
	engine Engine (1..1)
	vehicle Vehicle (1..1)

type Engine:
	quantity EngineMetric (1..1)
        [metadata location]
        [synonym MAPPERS_EXAMPLE_5 value "engineDetail" path "engineType"]

type Vehicle:
    resolvablePayoutQuantity EngineSpecification (1..1)
	    [synonym MAPPERS_EXAMPLE_5 value "engineDetail" path "engineType"]

type EngineSpecification:
	resolvedQuantity EngineMetric (1..1)
        [metadata address "pointsTo"=Engine->quantity]
        [synonym MAPPERS_EXAMPLE_5 value "horsePower" mapper "Example5"]

type EngineMetric:
	amount number (1..1)
	    [synonym MAPPERS_EXAMPLE_5 value "horsePower" maps 2]

