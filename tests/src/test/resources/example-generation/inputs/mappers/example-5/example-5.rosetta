namespace demo.translate.mappers.example_5
version "${project.version}"

/*
$DESCRIPTION$
*/
synonym source MAPPERS_EXAMPLE_5

type Root:
    [rootType]
	priceQuantity PriceQuantity (1..1)
	product Product (1..1)

type PriceQuantity:
	quantity Quantity (1..1)
        [metadata location]
        [synonym MAPPERS_EXAMPLE_5 value "b" path "a"]

type Product:
    resolvablePayoutQuantity ResolvablePayoutQuantity (1..1)
	    [synonym MAPPERS_EXAMPLE_5 value "b" path "a"]

type ResolvablePayoutQuantity:
	resolvedQuantity Quantity (1..1)
        [metadata address "pointsTo"=PriceQuantity->quantity]
        [synonym MAPPERS_EXAMPLE_5 value "c" mapper "Example5"]

type Quantity:
	amount number (1..1)
	    [synonym MAPPERS_EXAMPLE_5 value "c" maps 2]

