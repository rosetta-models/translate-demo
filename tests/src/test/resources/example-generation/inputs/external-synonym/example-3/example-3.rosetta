namespace demo.translate.external_synonym.example_3
version "${project.version}"

synonym source EXTERNAL_SYNONYM_EXAMPLE_3_BASE

synonym source EXTERNAL_SYNONYM_EXAMPLE_3 extends EXTERNAL_SYNONYM_EXAMPLE_3_BASE
{
	Root:
		+ partyTradeId
			[value "partyTradeIdentifier" path "a"]

	PartyTradeIdentifier3:
		+ partyEnum
			[value "partyReference"]

	enums

	PartyEnum3:
		+ FishParty
			[value "FISH"]
		+ ChipParty
			[value "CHIPS"]
}

type Root:
    [rootType]
    partyTradeId PartyTradeIdentifier3 (0..1)

enum PartyEnum3:
    FishParty
    ChipParty

type PartyTradeIdentifier3:
    partyEnum PartyEnum3 (1..*)
