namespace demo.translate.external_synonym.example_5
version "${project.version}"

synonym source EXTERNAL_SYNONYM_EXAMPLE_5_BASE

synonym source EXTERNAL_SYNONYM_EXAMPLE_5_MID extends EXTERNAL_SYNONYM_EXAMPLE_5_BASE
{
	AssignedIdentifier7:
		+ identifier
			[value "accountReference" meta "scheme"]
}

synonym source EXTERNAL_SYNONYM_EXAMPLE_5 extends EXTERNAL_SYNONYM_EXAMPLE_5_MID
{
	Contract7:
		+ id
			[value "partyTradeIdentifier" path "a"]

	AssignedIdentifier7:
		+ identifier
			[value "partyReference" meta "scheme"]
}

type Root:
    [rootType]
	contract Contract7 (1..1)

type Contract7:
	id Identifier7 (1..*)

type Identifier7:
	assignedId AssignedIdentifier7 (1..*)

type AssignedIdentifier7:
	identifier string (1..1)
		[metadata scheme]
