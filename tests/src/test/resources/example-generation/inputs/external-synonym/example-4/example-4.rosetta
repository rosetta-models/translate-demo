namespace demo.translate.external_synonym.example_4
version "${project.version}"

synonym source EXTERNAL_SYNONYM_EXAMPLE_4_BASE

synonym source EXTERNAL_SYNONYM_EXAMPLE_4_MID extends EXTERNAL_SYNONYM_EXAMPLE_4_BASE
{
	Root:
		+ partyTradeId
			[value "partyTradeIdentifier" path "a"]
}

synonym source EXTERNAL_SYNONYM_EXAMPLE_4 extends EXTERNAL_SYNONYM_EXAMPLE_4_MID
{
	PartyTradeIdentifier4:
		+ party
			[value "partyReference" meta "scheme"]
}

type Root:
    [rootType]
	partyTradeId PartyTradeIdentifier4 (0..1)

type PartyTradeIdentifier4:
	party string (1..*)
		[metadata scheme]
	account string (1..*)
		[metadata scheme]
		[synonym EXTERNAL_SYNONYM_EXAMPLE_4_BASE value "accountReference" meta "scheme"]