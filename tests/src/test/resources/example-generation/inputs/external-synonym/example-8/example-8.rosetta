			[value "e"]
}

type Root:
    [rootType]
	xField X8 (0..1)
	yField Y8 (0..1)

type Super8:
	superField string (0..1)

type X8 extends Super8:
	xxField string (1..1)

type Y8 extends Super8:
	yyField string (0..1)


