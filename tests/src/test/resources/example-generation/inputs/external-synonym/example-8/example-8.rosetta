namespace demo.translate.external_synonym.example_8
version "${project.version}"

synonym source EXTERNAL_SYNONYM_EXAMPLE_8

synonym source Bank_A8 extends EXTERNAL_SYNONYM_EXAMPLE_8
{
	Root:
		+ xField
			[value "a"]
		+ yField
			[value "a"]

	Super8:
		+ superField
			[value "b"]
			[value "d"]

	X8:
		+ xxField
			[value "c"]

	Y8:
		+ yyField
			[value "e"]
}

