synonym source Bank_A8_2 extends EXTERNAL_SYNONYM_EXAMPLE_8
{
	Root:
		+ xField
			[value "a"]
		+ yField
			[value "a"]

	Super8:
		+ superField
			[value "b" set when rosettaPath = Root -> xField -> superField]
